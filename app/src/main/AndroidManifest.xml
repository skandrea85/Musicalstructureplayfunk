<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andrea.musicalstructureplayfunk">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".rockActivity"
            android:label="Rock"
            android:parentActivityName="com.example.andrea.musicalstructureplayfunk.MainActivity"
            />
        <activity android:name=".reggaeActivity"
            android:label="Reggae"
            android:parentActivityName="com.example.andrea.musicalstructureplayfunk.MainActivity"
            />
        <activity android:name=".funkActivity"
            android:label="Funk"
            android:parentActivityName="com.example.andrea.musicalstructureplayfunk.MainActivity"
            />
        <activity android:name=".CountryActivity"
            android:label="Country"
            android:parentActivityName="com.example.andrea.musicalstructureplayfunk.MainActivity"
            ></activity>
    </application>

</manifest>